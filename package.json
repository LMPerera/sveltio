		"jest": "^27.2.5",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "^2.2.0",
		"svelte": "^3.42.2",
		"svelte-check": "^2.0.0",
		"svelte-jester": "^2.1.5",
		"svelte-loader": "^3.1.2",
		"svelte-preprocess": "^4.0.0",
		"svelte2tsx": "^0.4.7",
		"tslib": "^2.0.0",
		"typescript": "^4.0.0"
